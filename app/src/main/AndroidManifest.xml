<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.comcatertechnologies">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_new"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_new"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.comcatertechnologies.SplashActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.comcatertechnologies.MainActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/app_name"/>
    </application>

</manifest>